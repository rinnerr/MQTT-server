[Unit]
Description=Prometheus MySQL Exporter
After=network.target

[Service]
User=memcached
Group=memcached
Type=simple
ExecStart={{ memcached_exporter_dir }}/memcached_exporter --memcached.address={{ memcached_bind_address }}:{{ memcached_port }}
Restart=always

[Install]
WantedBy=multi-user.target
